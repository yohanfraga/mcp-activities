{
  "name": "mcp-activities",
  "version": "1.0.0",
  "description": "MCP server for creating activities in meupontoonline",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "inspector": "npx @modelcontextprotocol/inspector src/index.ts",
    "start:http": "npx mcp-proxy --port 3000 node dist/index.ts",
    "dev:http": "npx mcp-proxy --port 3000 --debug tsx src/index.ts"
  },
  "keywords": [
    "mcp",
    "activities",
    "cursor"
  ],
  "author": "Yohan Fraga Santos",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "formdata-node": "^6.0.3",
    "mcp-proxy": "^5.5.0",
    "tsx": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
